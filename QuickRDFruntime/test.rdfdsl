model
    namespace people "http://ses.sdk.dk/junk/people#"
        class Person 
            property olderThan Person
            property parentOf  Person
            property hasName   String
                cardinality 1.. 1
        class Male : Person
        class Female : Person
    namespace hestenet "https://www.hestenettet.dk"
    	class Hest
    		property hasLegs Integer
    			cardinality 1..4
data 
    namespace simpsons "http://ses.sdk.dk/junk/simpsons_data#"
    from "http://ses.sdk.dk/junk/people#" [Male, Female, olderThan, parentOf, hasName, hasAge]
        
        lisa, maggie, marge is Female
        bart, homer is Male

        homer: 
            hasName "Homer J. Simpson"
            olderThan marge, bart, lisa, maggie
            parentOf lisa, maggie, bart

        marge:
            hasName "Marge Simpson"
            olderThan bart, lisa, maggie
            parentOf lisa, maggie, bart

        bart:
            olderThan lisa, maggie
            hasName "Bart Simpson"
            
        lisa:
            olderThan maggie
            hasName "Lisa M. Simpson"
        
        maggie:
        	hasName "Maggie"